source: Planner/
properties:
  note.title:
    width: 200
  note.status:
    width: 100
  note.priority:
    width: 100
  note.date_due:
    width: 120
  note.calendar:
    width: 120
views:
  - type: planner-task-list
    name: Task List
    groupBy:
      property: calendar
      direction: ASC
