{
  "itemsFolder": "Planner/",
  "basesFolder": "Bases/",
  "itemTemplate": "Templates/Planner Template.md",
  "defaultCalendar": "Feature",
  "dateFormat": "YYYY-MM-DD",
  "timeFormat": "24h",
  "weekStartsOn": "monday",
  "statuses": [
    {
      "name": "Idea",
      "color": "#f7e955",
      "isCompleted": false,
      "icon": "circle-dashed"
    },
    {
      "name": "To-Do",
      "color": "#9c5ee8",
      "isCompleted": false,
      "icon": "circle-dot-dashed"
    },
    {
      "name": "In-Progress",
      "color": "#3b82f6",
      "isCompleted": false,
      "icon": "circle-dot"
    },
    {
      "name": "In-Review",
      "color": "#f97316",
      "isCompleted": false,
      "icon": "eye"
    },
    {
      "name": "Done",
      "color": "#22c55e",
      "isCompleted": true,
      "icon": "circle-check-big"
    },
    {
      "name": "Cancelled",
      "color": "#ef4444",
      "isCompleted": true,
      "icon": "ban"
    }
  ],
  "priorities": [
    {
      "name": "Urgent",
      "color": "#ef4444",
      "weight": 4,
      "icon": "signal"
    },
    {
      "name": "High",
      "color": "#f97316",
      "weight": 3,
      "icon": "signal-high"
    },
    {
      "name": "Medium",
      "color": "#eab308",
      "weight": 2,
      "icon": "signal-medium"
    },
    {
      "name": "Low",
      "color": "#3b82f6",
      "weight": 1,
      "icon": "signal-low"
    },
    {
      "name": "None",
      "color": "#6b7280",
      "weight": 0,
      "icon": "signal-zero"
    }
  ],
  "calendars": {
    "Feature": {
      "color": "#859900"
    },
    "Optimization": {
      "color": "#268bd2"
    },
    "Bugfix": {
      "color": "#dc322f"
    },
    "Research": {
      "color": "#d33682"
    },
    "Documentation": {
      "color": "#6c71c4"
    }
  },
  "calendarFontSize": 10,
  "quickCaptureHotkey": "Ctrl+Shift+N",
  "quickCaptureDefaultTags": [
    "task"
  ],
  "quickCaptureDefaultStatus": "To-Do",
  "quickCaptureOpenAfterCreate": false,
  "openBehavior": "new-tab"
}